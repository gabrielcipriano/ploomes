{
  "name": "ploomes",
  "version": "0.0.2",
  "description": "A Ploomes API Library for TypeScript and JavaScript applications, compatible with Node.js and web browser for easy interaction with the Ploomes API.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "lint": "tsc",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "upversion": "changeset",
    "release": "npm run build && changeset publish --verbose"
  },
  "dependencies": {
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": "^18.14",
    "npm": "^9.3"
  },
  "keywords": [
    "ploomes",
    "client",
    "api",
    "node",
    "sdk"
  ],
  "author": "Gabriel Cipriano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielcipriano/ploomes.git"
  },
  "bugs": {
    "url": "https://github.com/gabrielcipriano/ploomes/issues"
  },
  "homepage": "https://github.com/gabrielcipriano/ploomes#readme"
